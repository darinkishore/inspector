{
  "name": "@mcpjam/inspector",
  "version": "0.9.3",
  "type": "module",
  "description": "MCPJam Inspector",
  "license": "Apache-2.0",
  "author": "MCPJam (https://mcpjam.com)",
  "homepage": "https://mcpjam.com",
  "bugs": "https://github.com/mcpjam/inspector/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/MCPJam/inspector.git"
  },
  "main": "bin/start.js",
  "bin": {
    "inspector-vite": "bin/start.js"
  },
  "files": [
    "bin",
    "dist/client",
    "dist/server",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "start": "NODE_ENV=production node bin/start.js",
    "publish": "node -e \"console.error('Use npm publish directly.'); process.exit(1)\"",
    "prettier-fix": "prettier --write .",
    "install:deps": "cd server && npm install && cd ../client && npm install",
    "docker:build": "docker build -t mcpjam/mcp-inspector:latest .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t mcpjam/mcp-inspector:dev .",
    "docker:run": "docker run -p 3001:3001 mcpjam/mcp-inspector:latest",
    "docker:run:dev": "docker run -p 3001:3001 -p 5173:5173 mcpjam/mcp-inspector:dev",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose --profile dev up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "electron:publish": "electron-forge publish",
    "electron:dev": "electron-forge start",
    "test": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.23",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hono/node-server": "^1.13.7",
    "@hookform/resolvers": "^3.10.0",
    "@mastra/core": "^0.10.6",
    "@mastra/mcp": "^0.10.6",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@uiw/react-json-view": "^2.0.0-alpha.33",
    "ai": "^4.3.19",
    "ajv": "^8.17.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "electron-log": "^5.4.2",
    "embla-carousel-react": "^8.6.0",
    "fast-deep-equal": "^3.1.3",
    "fix-path": "^4.0.0",
    "framer-motion": "^12.23.6",
    "hono": "^4.6.11",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "ollama-ai-provider": "^1.2.0",
    "postcss": "^8.5.6",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.3",
    "react18-json-view": "^0.2.9",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "simple-icons": "^15.6.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "update-electron-app": "^3.1.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.2",
    "@electron-forge/maker-deb": "^7.8.2",
    "@electron-forge/maker-dmg": "^7.8.2",
    "@electron-forge/maker-rpm": "^7.8.2",
    "@electron-forge/maker-squirrel": "^7.8.2",
