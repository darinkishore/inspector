    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "electron": "^37.2.5",
    "eslint": "^9",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vite": "^6.0.7",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "protobufjs"
  ]
}
